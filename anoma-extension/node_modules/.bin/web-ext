#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/bin/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules:/Users/memas/development2/13_anoma-web/node_modules:/Users/memas/development2/node_modules:/Users/memas/node_modules:/Users/node_modules:/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/anoma-extension/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/bin/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/web-ext@6.6.0/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules:/Users/memas/development2/13_anoma-web/node_modules:/Users/memas/development2/node_modules:/Users/memas/node_modules:/Users/node_modules:/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/anoma-extension/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/bin/web-ext" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/web-ext@6.6.0/node_modules/web-ext/bin/web-ext" "$@"
fi

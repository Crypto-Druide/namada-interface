#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules/crx/src/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules/crx/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules:/Users/memas/development2/13_anoma-web/node_modules:/Users/memas/development2/node_modules:/Users/memas/node_modules:/Users/node_modules:/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/anoma-extension/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules/crx/src/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules/crx/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/crx@5.0.1/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules/.pnpm/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/node_modules:/Users/memas/development2/13_anoma-web/node_modules:/Users/memas/development2/node_modules:/Users/memas/node_modules:/Users/node_modules:/node_modules:/Users/memas/development2/13_anoma-web/anoma-web/anoma-extension/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/crx@5.0.1/node_modules/crx/src/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/crx@5.0.1/node_modules/crx/src/cli.js" "$@"
fi

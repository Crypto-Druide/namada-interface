{
  "manifest_version": 3,
  "name": "Anoma Extension",
  "version": "0.1.0",
  "background": {
    "service_worker": "background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://namada.me/*"],
      "js": ["content/index.ts"]
    }
  ],
  "action": {
    "default_popup": "static/popup.html"
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
